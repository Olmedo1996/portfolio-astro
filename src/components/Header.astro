---
import { NAV_DATA } from '../constants/navData.astro';
---
<header class="flex justify-center items-center py-5 w-full">
    <nav class="flex flex-row gap-x-10">
        {
            NAV_DATA.map(({name, link}) => (
                <a href={link}>{name}</a>
            ))
        }
    </nav>
</header>